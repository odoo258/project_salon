<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--sequence to bank payment code -->
        <record id="bank_payment_code" model="ir.sequence">
            <field name="name">Bank Payment Code</field>
            <field name="code">bank_payment_code_sequence</field>
            <field eval="1" name="number_next_actual"/>
            <field eval="1" name="number_increment"/>
            <field eval="10" name="padding"/>
        </record>

        <!--sequence to lot of payment code -->
        <record id="lot_payment_code" model="ir.sequence">
            <field name="name">Lot of Payment Code</field>
            <field name="code">lot_payment_code_sequence</field>
            <field eval="1" name="number_next_actual"/>
            <field eval="1" name="number_increment"/>
            <field eval="10" name="padding"/>
        </record>

        <!--sequence to our number santander  -->
        <record id="bank_payment_on_santander" model="ir.sequence">
            <field name="name">Bank Payment Our Number - Santander</field>
            <field name="code">bank_payment_our_number_santander</field>
            <field eval="1" name="number_next_actual"/>
            <field eval="1" name="number_increment"/>
            <field eval="7" name="padding"/>
        </record>

        <!--sequence number file header pag for santander  -->
        <record id="header_file_pagfor_santander" model="ir.sequence">
            <field name="name">Header File Pagfor Santander</field>
            <field name="code">header_file_pagfor_santander</field>
            <field eval="1" name="number_next_actual"/>
            <field eval="1" name="number_increment"/>
            <field eval="6" name="padding"/>
        </record>

        <!--sequence number file detail pag for santander  -->
        <record id="detail_file_pagfor_santander" model="ir.sequence">
            <field name="name">Detail File Pagfor Santander</field>
            <field name="code">detail_file_pagfor_santander</field>
            <field eval="1" name="number_next_actual"/>
            <field eval="1" name="number_increment"/>
            <field eval="8" name="padding"/>
        </record>
    </data>
</odoo>