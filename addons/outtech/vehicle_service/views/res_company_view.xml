<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_vehicle_company_form" model="ir.ui.view">
            <field name="name">br_account.company</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form" />
            <field name="arch" type="xml">
                <notebook position="inside">
                    <page string="API">
                        <group>
                            <field name="url_api" colspan="4"/>
                            <field name="active_api"/>
                            <field name="type_transition"/>
                            <field name="url_api_googlemaps" colspan="4"/>
                            <field name="radius_search"/>
                        </group>
                    </page>
                </notebook>
            </field>
        </record>
    </data>
</odoo>
